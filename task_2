class Student:

    def __init__(self, name, age, id):
        self.name = name
        self.age = age
        self.id = id
        self.subjects = dict()



class CFGStudent(Student):
    def __init__(self, name, id, subject)
        super().__init__(name,subject)



student_1 = Student ('Steve Harrington', 'Software'):
student_2 = Student ('Eddie Munson', 'Data Science'):
